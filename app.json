{
  "expo": {
    "name": "Elicit",
    "slug": "Elicit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android-chrome-512x512.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Elicit_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-chrome-512x512.ico",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/Elicit.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
